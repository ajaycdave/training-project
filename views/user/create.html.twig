{% extends "layout.twig" %}
{% block head %}

    {{ parent()}}
     <link href="assets/vendors/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css"/>
    <link href="assets/vendors/general/select2/dist/css/select2.css" rel="stylesheet" type="text/css"/>

{% endblock %}
{% block content %}


  <div class="row">
        <div class="col-lg-12">
            <div class="kt-portlet kt-margin-top-30">
                <div class="kt-portlet__head">
                    <div class="kt-portlet__head-label">
                        <h3 class="kt-portlet__head-title">
                            User Registration
                        </h3>
                    </div>
                </div>
                 {% if(status=="Error") %}
            <div class="alert alert-danger">{{ message}}</div>
            {% endif %}

                <!--begin::Form-->
            <form class="kt-form kt-form--label-right" action="user_registration.php?action=store" id="registrationForm" method="post" name="registrationForm" enctype="multipart/form-data">
                       
                    <div class="kt-portlet__body">
                         <div class="form-group row">
                            <div class="col-lg-6">
                            <label>First Name: <i class="text-danger">*</i></label>
                                <input type="text" id="first_name" name="first_name" class="form-control" value="{{first_name}}" placeholder="">
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-lg-6">
                            <label>Last Name: <i class="text-danger">*</i></label>
                                <input type="text" id="last_name" name="last_name" class="form-control" value="{{last_name}}" placeholder="">
                            </div>
                        </div>
                         <div class="form-group row">
                            <div class="col-lg-6">
                        <label>Email Address: <i class="text-danger">*</i></label>
                                <input type="text" id="email_address" name="email_address" class="form-control" value="{{email_address}}" placeholder="">
                            </div>
                        </div>
                         <div class="form-group row">
                            <div class="col-lg-6">
                        <label>Mobile:</label>
                                <input type="text" id="mobile" name="mobile" class="form-control" value="{{mobile}}" placeholder="">
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-lg-6">
                        <label>Password:</label>
                                <input type="password" id="password" name="password" class="form-control" value="" placeholder="">
                            </div>
                        </div>
                        <div class="form-group row">
                    <div class="col-lg-6"> <label>Confirm Password:</label>
                                <input type="password" id="conf_password" name="conf_password" class="form-control" value="" placeholder="">
                            </div>
                        </div>
                        
                    </div>
                    <div class="kt-portlet__foot">
                        <div class="kt-form__actions">
                            <div class="row">
                                <div class="offset-6 col-lg-6">
                                    <div class="pull-right">
                                        
                                          <button type="submit" class="btn btn-success"><i class="ik ik-check-circle" id="cl"></i>Save</button>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <!--end::Form-->
            </div>
        </div>
       
    </div>

    
{% endblock %}
{% block jspush %}

    <script src="assets/vendors/custom/datatables/datatables.bundle.js" type="text/javascript"></script>

  <script src="assets/js/user/create-user.js"></script>

      
{% endblock %}

{% block  footer%}
     {{ parent() }}
    
Footer add script

    

{% endblock %}